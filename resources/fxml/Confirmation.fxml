                                <Polygon fill="DODGERBLUE" stroke="BLACK" strokeType="INSIDE" styleClass="white-shape" StackPane.alignment="CENTER_LEFT">
                                    <points>
                                        <Double fx:value="0.0" />
                                        <Double fx:value="0.0" />
                                        <Double fx:value="-19.0" />
                                        <Double fx:value="18.0" />
                                        <Double fx:value="-19.0" />
