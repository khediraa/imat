                                    <children>
                                        <ImageView fitHeight="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../img/cartIcon_selected.png" />
                                            </image>
                                        </ImageView>
                                        <Label text="Varukorgen">
                                            <HBox.margin>
                                                <Insets right="30.0" />
                                            </HBox.margin>
                                        </Label>
                                    </children>
                                </HBox>
                            </children>
                        </StackPane>
                    </graphic>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
