                                        <Double fx:value="0.0" />
                                        <Double fx:value="0.0" />
                                        <Double fx:value="-19.0" />
                                        <Double fx:value="18.0" />
                                        <Double fx:value="-19.0" />
                                        <Double fx:value="-17.0" />
                                    </points>
                                </Polygon>
